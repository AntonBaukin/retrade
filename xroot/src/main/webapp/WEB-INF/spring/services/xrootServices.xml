<beans xmlns          = 'http://www.springframework.org/schema/beans'
   xmlns:xsi          = 'http://www.w3.org/2001/XMLSchema-instance'
   xsi:schemaLocation = 'http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd'>


  <!--=================================================
    =  Services List of XRoot Module                  =
    +================================================-->

  <bean name  = 'xrootServicesList'
        class = 'com.tverts.system.services.ServicesList'>
    <property name = 'references'>
      <list>

        <!-- · · · · · · · · · · · · · · · · · · · · · · · · · · · · ·
         · Execution Planning Service
         · · · · · · · · · · · · · · · · · · · · · · · · · · · · · -->
        <ref bean = 'execPlanService'/>


        <!-- · · · · · · · · · · · · · · · · · · · · · · · · · · · · ·
         · Execution Run Service
         · · · · · · · · · · · · · · · · · · · · · · · · · · · · · -->
        <ref bean = 'execRunService'/>


        <!-- · · · · · · · · · · · · · · · · · · · · · · · · · · · · ·
         · Security Service
         · · · · · · · · · · · · · · · · · · · · · · · · · · · · · -->
        <ref bean = 'secService'/>


        <!-- · · · · · · · · · · · · · · · · · · · · · · · · · · · · ·
         · Genesis Service
         · · · · · · · · · · · · · · · · · · · · · · · · · · · · · -->
        <ref bean = 'genesisService'/>


        <!-- · · · · · · · · · · · · · · · · · · · · · · · · · · · · ·
         · Self-Shunt Service
         · · · · · · · · · · · · · · · · · · · · · · · · · · · · · -->
        <ref bean = 'selfShuntService'/>


        <!-- · · · · · · · · · · · · · · · · · · · · · · · · · · · · ·
         · Reports Service
         · · · · · · · · · · · · · · · · · · · · · · · · · · · · · -->
        <ref bean = 'reportsService'/>


        <!-- · · · · · · · · · · · · · · · · · · · · · · · · · · · · ·
         · Models Store Service
         · · · · · · · · · · · · · · · · · · · · · · · · · · · · · -->
        <ref bean = 'modelsStoreService'/>


        <!-- · · · · · · · · · · · · · · · · · · · · · · · · · · · · ·
         · JavaScript Execution Service (JsX)
         · · · · · · · · · · · · · · · · · · · · · · · · · · · · · -->
        <ref bean = 'JsX'/>

      </list>
    </property>
  </bean>
</beans>