<beans xmlns          = 'http://www.springframework.org/schema/beans'
   xmlns:xsi          = 'http://www.w3.org/2001/XMLSchema-instance'
   xsi:schemaLocation = 'http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd'>


  <!--=================================================
    =  Ordering strategies of XRoot module            =
    +================================================-->

  <bean name  = 'xrootOrderers'
        class = 'com.tverts.endure.order.OrderersList'>

    <property name = 'references'>
      <list>

        <!-- · · · · · · · · · · · · · · · · · · · · · · · · · · · · ·
         · Used by ShuntOrdering.                                    ·
         ·                                                           ·
         · WARNING: do not change test orderer parameters as their   ·
         ·  values are assumed in the shunt unit!                    ·
         · · · · · · · · · · · · · · · · · · · · · · · · · · · · · -->
        <bean class = 'com.tverts.endure.order.OrdererTypeChecker'>

          <property name  = 'orderTypeClass'
                    value = 'com.tverts.endure.order.ExternalOrder'/>

          <property name  = 'orderTypeName'
                    value = 'Test: Domain: Order Index'/>

          <property name = 'orderer'>
            <bean class = 'com.tverts.endure.order.OrdererDefault'>

              <property name = 'insertStep'  value = '2'/>
              <property name = 'spreadLimit' value = '4'/>

            </bean>
          </property>
        </bean>

      </list>
    </property>
  </bean>
</beans>