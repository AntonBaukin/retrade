  <bean name  = 'retradeAggrTypes'
        class = 'com.tverts.objects.StringBean'>
    <property name = 'string'>
      <value>

        <!-- · · · · · · · · · · · · · · · · · · · · · · · · · · · · ·
         · Rest Cost of a Good (Unit)                                ·
         · · · · · · · · · · · · · · · · · · · · · · · · · · · · · -->
