    =  Unity Types of ReTrade Module                  =
    +==================================================
    |
    |  Format of System Unity Types encoding:
    |
    |    class-name type-name;
    |
    +================================================-->

  <bean name  = 'retradeUnityTypes'
        class = 'com.tverts.objects.StringBean'>
